<!-- membersPage.ejs -->
<!DOCTYPE html>
<html>
<head>
    <title>Members Page</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">

</head>
<body>
    <%-include('./partials/nav_redirect_home') %>

    <h1>Welcome to the Members Page</h1>
    <ul>
        <ul>
            <% messages.forEach(message => { %>
                <li>
                    <strong>Title:</strong> <%= message.title %><br>
                    <strong>Text:</strong> <%= message.text %><br>
                    <strong>Date:</strong> <%= message.date %><br>
                    <strong>Author:</strong> <%= message.author%><br>     
                </li>
            <% }) %>
        </ul>
        
        
    </ul>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">

</body>
</html>
